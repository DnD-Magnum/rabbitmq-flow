  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "php-etl/pipeline-contracts": "^0.3.0",
    "php-etl/bucket": "^0.2.0",
    "bunny/bunny": "*",
    "ramsey/uuid": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpunit/php-invoker": "*",
    "johnkary/phpunit-speedtrap": "*",
    "mybuilder/phpunit-accelerator": "*",
    "infection/infection": "^0.20",
    "adlawson/vfs": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
