  "autoload": {
    "psr-4": {
      "Kiboko\\Component\\Flow\\RabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "unit\\Kiboko\\Component\\Flow\\RabbitMQ\\": "tests/unit/",
      "functional\\Kiboko\\Component\\Flow\\RabbitMQ\\": "tests/functional/",
      "integration\\Kiboko\\Component\\Flow\\RabbitMQ\\": "tests/integration/"
    }
  },
  "config": {
    "bin-dir": "bin"
