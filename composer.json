{
  "name": "php-etl/rabbitmq-flow",
  "description": "RabbitMQ implementation for the ETL pipeline",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kiboko SAS",
      "homepage": "http://kiboko.fr"
    },
    {
      "name": "Gr√©gory Planchat",
